{
  "eslint.enable": false,
  "editor.formatOnSave": false,
  "editor.fontLigatures": false,
  "html-css-class-completion.enableEmmetSupport": false,
  "html.autoClosingTags": false,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
